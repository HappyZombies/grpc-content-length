    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Client",
            "program": "${workspaceFolder}//client.js",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
